{
    "name": "myrtle-backend",
    "description": "Robust Arknights API inspired by ArkPRTS.",
    "module": "index.ts",
    "dependencies": {
        "cheerio": "^1.0.0",
        "colors": "^1.4.0",
        "crypto-js": "^4.2.0",
        "dotenv": "^16.4.5",
        "eventemitter2": "latest",
        "ioredis": "^5.4.1",
        "lodash-es": "^4.17.21",
        "pg": "^8.13.1",
        "zod": "^3.23.8"
    },
    "devDependencies": {
        "@eslint/eslintrc": "^3.1.0",
        "@eslint/js": "^9.14.0",
        "@types/crypto-js": "^4.2.2",
        "@types/lodash-es": "^4.17.12",
        "@types/pg": "^8.11.10",
        "@typescript-eslint/eslint-plugin": "^8.14.0",
        "@typescript-eslint/parser": "^8.14.0",
        "@typescript-eslint/typescript-estree": "^8.14.0",
        "bun-types": "latest",
        "eslint": "9.14.0",
        "prettier": "^3.3.3",
        "tsc": "^2.0.4",
        "typescript-eslint": "^8.14.0"
    },
    "peerDependencies": {
        "typescript": "5.6.3"
    },
    "scripts": {
        "start": "bun run dist/index.js",
        "dev": "bun src/index.ts",
        "build": "bun build ./src/index.ts --outdir ./dist --target node",
        "prettier": "bunx prettier --write .",
        "lint": "bun run prettier && bunx eslint --fix .",
        "test:dps": "bun test src/tests/dps.test.ts",
        "export": "bun run src/scripts/export.ts",
        "import": "bun run src/scripts/import.ts",
        "download": "bun run src/scripts/assets-downloader.ts",
        "unpack": "bun run src/scripts/assets-unpacker.ts"
    },
    "type": "module"
}
